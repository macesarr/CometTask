{
    "manifest_version": 2,
    "name": "Comet-task",
    "description": "Extensión para guardar tareas de forma mas dinámica",
    "version": "1.0",
    "permissions" : ["https://*/*", "http://*/*"],
    "browser_action": {
	"name": "Comet-task",
	"icons": "icon.png",
	"default_icon": "icon-16.png",
	"default_popup": "popup.html"
    },
    "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
